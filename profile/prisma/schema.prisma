// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile{
  ProfileId Int  @default(autoincrement())
  Image String
  userId String @id
  phone_number Int
  address String
  city String
  state String
  country String
  pincode Int

}